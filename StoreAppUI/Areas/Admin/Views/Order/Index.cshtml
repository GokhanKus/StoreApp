@model IEnumerable<Order>

@if (Model.Count() > 0)
{
	@foreach (var order in Model)
	{
		<div class="my-3">
			<partial name="_OrderDetails" model="@order"></partial> @*sadece bu sayfada _OrderDetailsi kullanacagim icin _OrderDetails.cshtml sayfasini Order klasoru altinda olusturabilirim.*@
		</div>
	}
}
else
{
	<div class="alert alert-warning">
		<p class="lead">There is no order yet</p>
	</div>
}